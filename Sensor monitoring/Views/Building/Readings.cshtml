@model IEnumerable<Sensor_monitoring.Models.Readings>


<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Value</th>
            <th scope="col">Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var reading in Model)
        {
            <tr>
                <th> @reading.ReadingValue </th>
                <td>@reading.Date</td>
            </tr>
        }
    </tbody>
</table>

